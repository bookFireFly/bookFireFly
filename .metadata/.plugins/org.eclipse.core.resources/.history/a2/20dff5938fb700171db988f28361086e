	rtn=date.indexOf("-",4);
	console.log (rtn);
	rtn=4
	rtn=date.indexOf("-",5);
	console.log (rtn);
	rtn=7
	
	rtn=date.indexOf("2",0);
	console.log (rtn);
	rtn=0
	rtn=date.indexOf("2",1);
	console.log (rtn);
	rtn=8